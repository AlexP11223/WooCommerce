{
  "name": "mollie/mollie-woocommerce",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Mollie B.V.",
      "email": "info@mollie.com"
    },
    {
      "name": "Inpsyde Gmbh",
      "email": "hello@inpsyde.com"
    }
  ],
  "require": {
    "php": "^5.6.40 | ^7.0",
    "mollie/mollie-api-php": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5 || ^6.5 || ^7.1",
    "brain/monkey": "^2.3",
    "ptrofimov/xpmock": "^1",
    "johnpbloch/wordpress-core": "^3.8 || ^4.0 || ^5.0",
    "woocommerce/woocommerce": "^2.2 || ^3.0"
  },
  "autoload": {
    "psr-0": {
      "Mollie_WC_": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mollie\\WooCommerceTests\\": "tests/php",
      "Mollie\\WooCommerceTests\\Unit\\": "tests/php/Unit",
      "Mollie\\WooCommerceTests\\Functional\\": "tests/php/Functional"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-disable": true
  },
  "config": {
    "platform": {
      "php": "5.6.40"
    }
  }
}
