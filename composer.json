{
    "name": "mollie/mollie-woocommerce",
    "type": "wordpress-plugin",
    "license": "GLP-v2",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5 || ^6.5 || ^7.1",
        "brain/monkey": "^2.3",
        "ptrofimov/xpmock": "^1"
    },
    "autoload": {
        "classmap": [
            "mollie-payments-for-woocommerce/includes"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mollie\\WooCommerce\\Tests\\": "tests/php",
            "Mollie\\WooCommerce\\Tests\\Unit\\": "tests/php/Unit",
            "Mollie\\WooCommerce\\Tests\\Functional\\": "tests/php/Functional"
        }
    }
}
